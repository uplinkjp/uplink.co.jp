<%
  if(typeof theme === 'undefined') { var theme = 'theme-main' }
  if(typeof page === 'undefined') { var page = '' }
%>

<html>
<head>
  <meta charset="UTF-8">
  <meta name="author" content="著者情報">
  <meta name="description" content="記事の概要">
  <meta name="format-detection" content="telephone=no,address=no,email=no">
  <meta name="viewport" content="width=device-width,initial-scale=1.0">

  <meta name="twitter:card" content="summary">
  <meta name="twitter:site" content="@twitter">
  <meta property="fb:app_id" content="00000000000">
  <meta property="og:description" content="記事の概要">
  <meta property="og:title" content="UPLINK">
  <meta property="og:url" content="http://www.uplink.co.jp/">
  <meta property="og:image" content="http://www.uplink.co.jp/img/ogp.png">
  <meta property="og:type" content="website">
  <meta property="og:site_name" content="UPLINK">

<% if(theme === 'theme-main') { %>
  <link rel="icon" href="./img/icon/uplink/favicon-16.png" sizes="16x16">
  <link rel="icon" href="./img/icon/uplink/favicon-32.png" sizes="32x32">
  <link rel="icon" href="./img/icon/uplink/favicon-48.png" sizes="48x48">
  <link rel="icon" href="./img/icon/uplink/favicon-62.png" sizes="62x62">
  <link rel="icon" href="./img/icon/uplink/favicon-192.png" sizes="192x192">
  <link rel="apple-touch-icon" href="./img/icon/uplink/apple-touch-icon.png" sizes="180x180">
<% } else if(theme === 'theme-joji') { %>
  <link rel="icon" href="./img/icon/uplink_joji/favicon-16.png" sizes="16x16">
  <link rel="icon" href="./img/icon/uplink_joji/favicon-32.png" sizes="32x32">
  <link rel="icon" href="./img/icon/uplink_joji/favicon-48.png" sizes="48x48">
  <link rel="icon" href="./img/icon/uplink_joji/favicon-62.png" sizes="62x62">
  <link rel="icon" href="./img/icon/uplink_joji/favicon-192.png" sizes="192x192">
  <link rel="apple-touch-icon" href="./img/icon/uplink_joji/apple-touch-icon.png" sizes="180x180">
<% } else if(theme === 'theme-shibuya') { %>
  <link rel="icon" href="./img/icon/uplink_shibuya/favicon-16.png" sizes="16x16">
  <link rel="icon" href="./img/icon/uplink_shibuya/favicon-32.png" sizes="32x32">
  <link rel="icon" href="./img/icon/uplink_shibuya/favicon-48.png" sizes="48x48">
  <link rel="icon" href="./img/icon/uplink_shibuya/favicon-62.png" sizes="62x62">
  <link rel="icon" href="./img/icon/uplink_shibuya/favicon-192.png" sizes="192x192">
  <link rel="apple-touch-icon" href="./img/icon/uplink_shibuya/apple-touch-icon.png" sizes="180x180">
<% } %>

  <link rel="alternate" type="application/rss+xml" href="http://example.com/feed">
  <link rel="stylesheet" href="./css/style.min.css">

  <title>UPLINK</title>

</head>
<body class="<%- theme; %> <%- page; %>">
  <div id="fb-root"></div>
  <script>(function(d, s, id) {
    var js, fjs = d.getElementsByTagName(s)[0];
    if (d.getElementById(id)) return;
    js = d.createElement(s); js.id = id;
    js.src = "//connect.facebook.net/ja_JP/sdk.js#xfbml=1&version=v3.2";
    fjs.parentNode.insertBefore(js, fjs);
  }(document, 'script', 'facebook-jssdk'));</script>